FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          incompressibleFluid;

startFrom       startTime;

startTime       0.869;

stopAt          endTime;

endTime         1.0;

deltaT          0.0005;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   4;

writeControl adjustableRunTime;

writeControl adjustableRunTime;

maxCo           0.3;

runTimeModifiable true;


